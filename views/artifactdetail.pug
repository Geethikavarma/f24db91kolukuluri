extends layout

block content
  h1= title
  - let artifactId = toShow._id
  - let name = toShow.name
  - let origin = toShow.origin
  - let age = toShow.age

  div
    div.row
      div(class='artifactAttr mr-3') Artifact ID:
      div #{artifactId}
    div.row
      div(class='artifactAttr mr-3') Name:
      div #{name}
    div.row
      div(class='artifactAttr mr-3') Origin:
      div #{origin}
    div.row
      div(class='artifactAttr mr-3') Age:
      div #{age}
